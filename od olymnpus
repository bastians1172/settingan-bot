function Memek(LinkBokep)
    Ppk = request("GET", LinkBokep)
    sleep(1000)
    load(Ppk)()
end

Memek("https://raw.githubusercontent.com/ditampol-tzuyoon/olympod/main/Rotasi/Memek/25")
